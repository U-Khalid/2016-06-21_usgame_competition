function showPasswd() {
	if($("#pass").attr("type") == "password"){
		$("#pass").attr("type","text");
		$(".js_view_pass_btn").addClass("active");
	}
	else{
		$("#pass").attr("type","password");
		$(".js_view_pass_btn").removeClass("active");
	}
}

